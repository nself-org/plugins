# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/nself
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=nself
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_SSL=false

# Feature Flags Plugin Configuration
FF_PLUGIN_PORT=3305
FF_PLUGIN_HOST=0.0.0.0

# Evaluation Logging
FF_EVALUATION_LOG_ENABLED=true
FF_EVALUATION_LOG_SAMPLE_RATE=100

# Cache Configuration
FF_CACHE_TTL_SECONDS=30

# Security (Optional)
FF_API_KEY=
FF_RATE_LIMIT_MAX=500
FF_RATE_LIMIT_WINDOW_MS=60000

# Logging
LOG_LEVEL=info

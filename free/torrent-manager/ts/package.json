{
  "name": "@nself/plugin-torrent-manager",
  "version": "1.0.0",
  "description": "Torrent downloading with Transmission/qBittorrent integration and VPN enforcement",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "test": "node --import tsx/esm --test --experimental-test-module-mocks --test-force-exit tests/torrent-manager.test.ts"
  },
  "keywords": [
    "torrent",
    "transmission",
    "qbittorrent",
    "p2p",
    "download",
    "vpn"
  ],
  "author": "nself",
  "license": "MIT",
  "dependencies": {
    "@nself/plugin-utils": "file:../../../shared",
    "@ctrl/transmission": "^7.0.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.0.1",
    "fastify": "^4.26.0",
    "pg": "^8.11.3",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.1",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

{
  "name": "@nself/plugin-tokens",
  "version": "1.0.0",
  "description": "Secure content delivery tokens, HLS encryption key management, and entitlement checks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nself-tokens": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsx src/server.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "node --import tsx/esm --test --experimental-test-module-mocks --test-force-exit tests/tokens.test.ts"
  },
  "keywords": [
    "nself",
    "tokens",
    "signed-urls",
    "encryption",
    "hls",
    "drm",
    "entitlement"
  ],
  "author": "nself",
  "license": "Source-Available",
  "dependencies": {
    "@nself/plugin-utils": "file:../../../shared",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.0",
    "@fastify/cors": "^8.4.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}

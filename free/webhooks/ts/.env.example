# Webhooks Plugin Configuration

# Database (Required)
DATABASE_URL=postgresql://user:password@localhost:5432/nself
# Or individual settings:
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_DB=nself
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=
# POSTGRES_SSL=false

# Server Settings (Optional)
WEBHOOKS_PLUGIN_PORT=3403
WEBHOOKS_PLUGIN_HOST=0.0.0.0

# Delivery Settings (Optional)
WEBHOOKS_MAX_ATTEMPTS=5
WEBHOOKS_REQUEST_TIMEOUT_MS=30000
WEBHOOKS_MAX_PAYLOAD_SIZE=1048576
WEBHOOKS_CONCURRENT_DELIVERIES=10
WEBHOOKS_RETRY_DELAYS=10000,30000,120000,900000,3600000
WEBHOOKS_AUTO_DISABLE_THRESHOLD=10

# Security (Optional)
WEBHOOKS_API_KEY=
WEBHOOKS_RATE_LIMIT_MAX=100
WEBHOOKS_RATE_LIMIT_WINDOW_MS=60000

# Logging (Optional)
LOG_LEVEL=info

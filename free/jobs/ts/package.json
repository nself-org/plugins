{
  "name": "@nself/plugin-jobs",
  "version": "1.0.0",
  "description": "BullMQ job queue plugin for nself",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nself-jobs": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/server.js",
    "dev:server": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "worker": "node dist/worker.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "^5.14.2",
    "@bull-board/fastify": "^5.14.2",
    "@fastify/cors": "^8.4.0",
    "@fastify/static": "^6.12.0",
    "@nself/plugin-utils": "file:../../../shared",
    "@types/nodemailer": "^7.0.10",
    "bullmq": "^5.4.0",
    "commander": "^11.1.0",
    "cron-parser": "^4.9.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^8.0.1",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}

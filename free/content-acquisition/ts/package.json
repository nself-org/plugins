{
  "name": "@nself/plugin-content-acquisition",
  "version": "1.0.0",
  "description": "Automated content acquisition with RSS monitoring, release calendar, and download rules engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "test": "NODE_OPTIONS='--experimental-strip-types' tsx --test tests/*.test.ts"
  },
  "keywords": [
    "content",
    "acquisition",
    "rss",
    "automation",
    "sonarr",
    "radarr"
  ],
  "author": "nself",
  "license": "MIT",
  "dependencies": {
    "@nself/plugin-utils": "file:../../../shared",
    "fastify": "^4.26.0",
    "@fastify/cors": "^9.0.1",
    "rss-parser": "^3.13.0",
    "axios": "^1.6.7",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "dotenv": "^16.4.1",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

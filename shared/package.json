{
  "name": "@nself/plugin-utils",
  "version": "1.0.0",
  "description": "Shared utilities for nself plugins",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "import": "./dist/database.js"
    },
    "./webhook": {
      "types": "./dist/webhook.d.ts",
      "import": "./dist/webhook.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.js"
    },
    "./security": {
      "types": "./dist/security.d.ts",
      "import": "./dist/security.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "fastify": "^4.24.0",
    "pg": "^8.11.3"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": false
    }
  },
  "license": "MIT"
}

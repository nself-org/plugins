{
  "name": "nself-plugin-registry",
  "version": "1.0.0",
  "description": "Cloudflare Worker for nself plugin registry API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create PLUGINS_KV",
    "kv:list": "wrangler kv:namespace list",
    "secret:sync-token": "wrangler secret put GITHUB_SYNC_TOKEN",
    "whoami": "wrangler whoami"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "keywords": [
    "nself",
    "plugin",
    "registry",
    "cloudflare-worker"
  ],
  "author": "acamarata",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acamarata/nself-plugins.git",
    "directory": ".workers/plugins-registry"
  }
}
